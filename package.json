{
  "name": "portfolio-2",
  "version": "0.0.1",
  "description": "Portfolio 2017",
  "author": "Sam Garson <samtgarson@gmail.com>",
  "private": true,
  "dependencies": {
    "decouple": "^1.0.0",
    "nuxt": "latest",
    "poisson-disk-sampling": "^1.0.1"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "xo",
    "ci": "xo --reporter junit"
  },
  "devDependencies": {
    "eslint-config-samtgarson": "^0.0.5",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-html": "^2.0.0",
    "node-sass": "^4.5.0",
    "now-pipeline": "^1.7.1",
    "pug": "2",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.1.1",
    "sass-variable-loader": "^0.0.4",
    "xo": "^0.17.1"
  },
  "now": {
    "aliases": [
      "samtgarson"
    ]
  },
  "xo": {
    "extends": "samtgarson",
    "extensions": [
      "vue"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "plugins": [
      "html"
    ],
    "semicolon": false,
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "import/no-webpack-loader-syntax": 0
    }
  }
}
